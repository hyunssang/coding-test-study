public class Day_2_2 {
    public static void main(String[] args) {
        Solution solution = new Day_2_2().new Solution();
        int num1 = 10;
        int num2 = 3;
        int result = solution.solution(num1, num2);
        System.out.println(result);
    }

    class Solution {
        public int solution(int num1, int num2) {
            return num1 % num2;
        }
    }
}
