class Solution {
    public long[] solution(int x, int n) {
        long[] answer = new long[n];

        
        if(x>= -10000000 && x<=10000000 && n>=0 && n<=1000){
            for(int i=0; i<answer.length; i++){
               answer[i] = (long)x * (i + 1);
            }
        }
        return answer;
    }
}
